<NETWORK>
	<CONSTRAINT type="bound_loop" />
	<SPECIE name="A">
		<REGUL source="B" threshold="1" />
		<REGUL source="A" />            
		<CONSTR expr="A:0 = 0" />
		<CONSTR expr="B = 1" />
		<CONSTR expr="A = 0" />
		<CONSTR expr="A,B = 0"/>   
	</SPECIE>
	<SPECIE name="B">
		<REGUL source="A" />
		<REGUL source="B" />            
		<CONSTR expr="A,B = A:0 & A:1 > 0" />
	</SPECIE>
</NETWORK>